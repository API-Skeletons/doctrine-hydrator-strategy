{
    "name": "api-skeletons/zf-doctrine-hydrator",
    "description": "A collection of common hydrators for phpro/zf-doctrine-hydration-module",
    "type": "library",
    "require": {
        "php": "^7.1",
        "phpro/zf-doctrine-hydration-module": "^3.0",
        "laminas/laminas-eventmanager": "^2.4 || ~3.0",
        "laminas-api-tools/api-tools-doctrine": "^2.0",
        "laminas/laminas-servicemanager": "^3.0",
        "laminas/laminas-modulemanager": "^2.7",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*",
        "laminas-api-tools/api-tools": "*",
        "laminas-api-tools/api-tools-configuration": "*",
        "doctrine/doctrine-orm-module": "*",
        "doctrine/data-fixtures": "*",
        "laminas/laminas-test": "*",
        "laminas/laminas-serializer": "*",
        "laminas/laminas-log": "*",
        "laminas/laminas-config": "*",
        "laminas/laminas-console": "*",
        "laminas/laminas-i18n": "*",
        "api-skeletons/coding-standard": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tom H Anderson",
            "email": "tom.h.anderson@gmail.com"
        },
        {
            "name": "Marcelo Insabralde",
            "email": "marcelominsabralde@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ZF\\Doctrine\\Hydrator\\": "src/",
            "ZFTest\\Doctrine\\Hydrator\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "extra": {
        "zf": {
            "module": "ZF\\Doctrine\\Hydrator"
        }
    }
}
